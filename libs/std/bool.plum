module std

Bool =
  | True
  | False

# Might not be needed
Bool\toStr() -> Str =
  if this == True
    "True"
  else
    "False"

Bool\and(other: Bool) -> Bool =
  match this, other is
    True, True => True
    True, False => False
    False, True => False
    False, False => False

Bool\or(other: Bool) -> Bool =
  match this, other is
    True, True => True
    True, False => True
    False, True => True
    False, False => False